/* components/search/search.wxss */
.search-link{
  width:100%;
}
.top-search{
  height:100rpx;
  width:100%;
  background-color:#875f3f;
  margin:0;
  display: flex;
  justify-content: center;
  align-items: center;
  padding:0 40rpx;
  box-sizing:border-box;
  position:fixed;
  top:0px;
  z-index:10;
}
.me-search{
  width:100%;
  height:68rpx;
  background-color: white;
  border-radius:34rpx;
  box-sizing:border-box;
  padding-left:40rpx;
  font-size:30rpx;
  display:flex;
  justify-content: center;
  align-items: center;
  color:#999;
}
.me-search>image{
  width:42rpx;
  height:42rpx;
  padding-right:18rpx;
}
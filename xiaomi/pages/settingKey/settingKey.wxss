/* pages/address/address.wxss */
page{
  background-color: #fed;
}
.uname{
  font-size:20px;
  font-weight:600;
}
.key{
  margin-top:30rpx;
  border-radius: 20rpx;
  background-color: #fff;
  padding:20rpx;
}

.submit{
  border-radius: 0;
  position:fixed;
  bottom:0px;
  left:0px;
  width:100%;
  height:100rpx;
  display:flex;
  justify-content: center;
  align-items: center;
  background-color: #2a1;
  color:#fff;
  font-size:20px;
}
.key-item{
  display: flex;
  justify-items: flex-start;
  padding:18rpx;
}
.key-item text{
  width:180rpx;
  margin-top:6rpx;
}
.key-item input{
  border-bottom:1px solid #999;
  padding-left:10rpx;
}